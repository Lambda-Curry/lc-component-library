{
  "name": "@lambdacurry/component-library",
  "version": "0.2.15",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "description": "A React component library created by Lambda Curry.",
  "scripts": {
    "prestart": "yarn build:tailwind",
    "prebuild": "yarn build:tailwind",
    "prebuild:library": "yarn build:tailwind && rm -rf ./dist",
    "postbuild:library": "cpy '**/*' '!**/{*.ts*,*.scss,*.mdx}' ../../dist --cwd=src/lib --no-overwrite --parents",
    "prestorybook": "yarn build:tailwind",
    "prebuild:storybook": "yarn build:tailwind",
    "start": "concurrently \"yarn watch:tailwind\" \"react-scripts start\"",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "clean": "rm -rf ./node-modules && yarn",
    "storybook": "concurrently \"yarn watch:tailwind\" \"start-storybook -p 6007\"",
    "build:storybook": "build-storybook -o docs",
    "build:tailwind": "postcss src/styles/index.scss -o src/styles/tailwind.scss",
    "watch:tailwind": "chokidar 'src/**/*.css' 'src/**/*.scss' --ignore src/styles/tailwind.scss -c 'yarn build:tailwind'",
    "build:library": "rollup --config"
  },
  "dependencies": {
    "@material-ui/core": "^4.11.0",
    "@material-ui/lab": "^4.0.0-alpha.56",
    "classnames": "^2.2.6",
    "formik": "^2.1.5",
    "framer-motion": "^2.6.3",
    "lodash": "^4.17.20",
    "node-sass": "^4.14.1",
    "notistack": "^1.0.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-modal": "^3.11.2",
    "react-quill": "2.0.0-beta.2",
    "react-scripts": "3.4.3",
    "tailwindcss": "^1.7.3",
    "typescript": "~3.7.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.11.0",
    "@babel/preset-typescript": "^7.10.4",
    "@material-ui/types": "^5.1.0",
    "@rollup/plugin-commonjs": "^15.0.0",
    "@rollup/plugin-node-resolve": "^9.0.0",
    "@rollup/plugin-typescript": "^5.0.2",
    "@storybook/addon-a11y": "^6.0.21",
    "@storybook/addon-actions": "^6.0.21",
    "@storybook/addon-controls": "^6.0.21",
    "@storybook/addon-docs": "^6.0.21",
    "@storybook/addon-info": "^5.3.19",
    "@storybook/addon-links": "^6.0.21",
    "@storybook/addon-notes": "^5.3.19",
    "@storybook/addon-storyshots": "^6.0.21",
    "@storybook/addon-storyshots-puppeteer": "^6.0.21",
    "@storybook/addon-storysource": "^6.0.21",
    "@storybook/addon-viewport": "^6.0.21",
    "@storybook/addons": "^6.0.21",
    "@storybook/preset-create-react-app": "^3.1.4",
    "@storybook/react": "^6.0.21",
    "@svgr/rollup": "^5.4.0",
    "@svgr/webpack": "^5.4.0",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "@types/classnames": "^2.2.10",
    "@types/jest": "^24.0.0",
    "@types/lodash": "^4.14.160",
    "@types/node": "^12.0.0",
    "@types/react": "^16.9.0",
    "@types/react-dom": "^16.9.0",
    "@types/react-modal": "^3.10.6",
    "babel-plugin-named-asset-import": "^0.3.6",
    "babel-plugin-react-require": "^3.1.3",
    "babel-plugin-remove-graphql-queries": "^2.9.17",
    "chokidar-cli": "^2.1.0",
    "concurrently": "^5.3.0",
    "cpy-cli": "^3.1.1",
    "eslint-plugin-mdx": "^1.8.2",
    "postcss-cli": "^7.1.1",
    "postcss-import": "^12.0.1",
    "postcss-scss": "^2.1.1",
    "rollup": "^2.26.5",
    "rollup-plugin-postcss": "^3.1.6",
    "rollup-plugin-scss": "^2.6.0",
    "rollup-plugin-tailwind": "^1.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-url": "^3.0.1",
    "string-dedent": "^1.1.1",
    "url-loader": "^4.1.0"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "plugin:mdx/recommended"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Lambda-Curry/lc-component-library.git"
  },
  "keywords": [
    "react",
    "component",
    "library"
  ],
  "author": "Lambda Curry",
  "license": "ISC",
