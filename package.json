    "react-dom": "17.0.1",
    "react-input-mask": "^2.0.4",
    "react-modal": "^3.12.1",
    "react-quill": "2.0.0-beta.2",
    "react-scripts": "4.0.1",
    "react-text-mask": "^5.4.3",
